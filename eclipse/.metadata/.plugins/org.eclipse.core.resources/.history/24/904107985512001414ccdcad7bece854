package com.kenny2810.mysteriousCookies;

import net.minecraft.entity.player.EntityPlayer;
import net.minecraft.item.ItemFood;
import net.minecraft.item.ItemStack;
import net.minecraft.potion.Potion;
import net.minecraft.potion.PotionEffect;
import net.minecraft.world.World;

public class mysterious_Cookie extends ItemFood{

	public mysterious_Cookie(int food, boolean feedtonimals){
		super(food, feedtonimals);
		// TODO Auto-generated constructor stub
	}
	
	 protected void onFoodEaten(ItemStack p_77849_1_, World p_77849_2_, EntityPlayer p_77849_3_)
	    {
	            if (!p_77849_2_.isRemote)
	            {
	            	int potion = 0;
	            switch(potion){
	            case 1:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 2));break;
	            case 2:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.poison.id, 600, 4));break;
	            case 3:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.damageBoost.id, 600, 4));break;
	            case 4:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.blindness.id, 600, 4));break;
	            case 5:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 6:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 7:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 8:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 9:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 10:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 11:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 12:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 13:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 14:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 15:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            case 16:  p_77849_3_.addPotionEffect(new PotionEffect(Potion.regeneration.id, 600, 4));break;
	            
	            }
	            }
	        
	        else
	        {
	            super.onFoodEaten(p_77849_1_, p_77849_2_, p_77849_3_);
	        }
	    }

}
